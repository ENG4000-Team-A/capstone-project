server {
    listen 80 default_server;
    server_name _;
    root /var/www/consoletracker;
    index index.html index.htm;	
    
    location / {
        try_files $uri $uri/ =404;
    }

}


server {
    listen 8000;
    server_name _;

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }

}

server { 
    listen localhost:81;
    location /metrics {
    	stub_status on;
    }
}


